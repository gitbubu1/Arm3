// to check wheter a number is amrstrong or not

void main()
{
    int e,f,g,h=0;
    printf("Enter a number to check Armstrong or not \n");
    scanf("%d",&e);
    f=e;
    while(f!=0)
    {
        g=f%10;
        h=h+(g*g*g);
        f=f/10;
    }
    if(h==e)
    {
        printf("%d is Armstrong number ",e);
    }
    else{
        printf("%d is not an Armstrong number ",e);
    }
    getch();
}
